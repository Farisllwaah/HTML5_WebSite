<slide_show>
	<options>
		<debug>false</debug>						<!-- true, false -->
		<background>FFFFFF</background>				<!-- 0xRRGGBB or 0x000000 -->

		<interaction>
			<rotation>mouse</rotation>				<!-- auto, mouse, keyboard -->
			<speed>60</speed>						<!-- [-360,360] -->
			<view_point>mouse</view_point>			<!-- none, mouse, keyboard -->
			<console>onClick</console>				<!-- show, hide, onClick -->
		</interaction>

		<far_photos>
			<size>25</size>						<!-- [0,100] -->
			<amount>75</amount>					<!-- [0,100] transparency-->
			<transparency>true</transparency>			<!-- true, false -->
		</far_photos>

		<reflection>
			<amout>100</amout>					<!-- [0,1000] -->
			<blur>5</blur>						<!-- [0,100] -->
			<distance>0</distance>					<!-- [-1000,1000] -->
			<alpha>75</alpha>						<!-- [0,100] -->
		</reflection>

	</options>

